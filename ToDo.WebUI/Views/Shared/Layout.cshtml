@{
    Layout = null;
}

<!DOCTYPE html>

<html manifest="todo.appcache">

<head>
    <meta name="viewport" content="width=device-width" />
    <script src="~/Scripts/libs/jquery-2.1.3.js" type="text/javascript"></script>
    <script src="~/Scripts/libs/knockout-3.2.0.js" type="text/javascript"></script>


    <link href="~/Content/Bootstrap/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/customize/customize.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/libs/Moment/moment.min.js" type="text/javascript"></script>
    <script src="~/Scripts/libs/knockout.bindings.date.js" type="text/javascript"></script>
    <script src="~/Scripts/libs/Bootstrap/bootstrap.min.js"></script>
    <link href="~/Scripts/libs/jquery-ui-1.11.3.custom/jquery-ui.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/libs/jquery-ui-1.11.3.custom/jquery-ui.min.js" type="text/javascript"></script>
    <title>@ViewBag.Title</title>

    @RenderSection(name: "Scripts", required: false)
</head>
<body>
    <header class="navbar navbar-static-top " role="navigation">
        <section class="container">
            <section class="navbar-header">
                <a class="navbar-brand" href="~/taskoard">ToDo</a>
            </section>
            @{
                Html.RenderAction("LoginHeader", "Shared");
            }
        </section>
    </header>

    <section class="container">
        @RenderBody()
    </section>


    <footer></footer>
</body>
</html>
